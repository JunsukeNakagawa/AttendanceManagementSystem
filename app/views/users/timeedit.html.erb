<%= form_for @user, :url => {:action => 'timeupdate'}, method: "post" do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :指定勤務時間 %>
  <%= f.time_field :workingtime, class: 'form-control' %>
  <%= f.label :基本勤務時間 %>
  <%= f.time_field :basictime, class: 'form-control' %>
  <%= f.submit "保存", class: "btn btn-primary" %>
<% end %>